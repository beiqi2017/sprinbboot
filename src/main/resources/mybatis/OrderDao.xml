<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.dao.OrderDao">

    <select id="get" resultType="com.example.demo.domain.Order">
        select * from t_order
    </select>
    
    <insert id="add" parameterType="com.example.demo.domain.Order" keyProperty="uid" useGeneratedKeys="true">
	     insert into t_order(id,order_id,user_id)  VALUES (#{id},#{order_id},#{user_id})
	 </insert>

</mapper>